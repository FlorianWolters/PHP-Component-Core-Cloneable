{
    "name": "florianwolters/component-core-cloneable",
    "description": "Allows and disallows the cloning of objects as a PHP component.",
    "type": "library",
    "keywords": ["cloning", "clone", "immutable", "creation", "core"],
    "homepage": "http://github.com/FlorianWolters/PHP-Component-Core-Cloneable",
    "license": "LGPL-3.0+",
    "authors": [
      {
        "name": "Florian Wolters",
        "email": "wolters.fl@gmail.com",
        "homepage": "http://blog.florianwolters.de",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.*",
        "pdepend/pdepend": "2.0.*",
        "phpmd/phpmd": "2.0.*",
        "phpunit/phpunit": "4.1.*",
        "sebastian/phpcpd": "2.0.*",
        "sebastian/phpdcd": "1.0.*",
        "satooshi/php-coveralls": "dev-master",
        "sensiolabs/security-checker": "1.3.*",
        "squizlabs/php_codesniffer": "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "FlorianWolters\\Component\\Core\\": "src/main/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlorianWolters\\Component\\Core\\": "src/test/php",
            "FlorianWolters\\Example\\": "src/test/resources"
        }
    }
}
